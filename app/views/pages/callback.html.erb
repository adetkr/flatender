    <% if @last_signed_contracts.first.match.flat1.user == current_user %>
      <% current_user_flat = @last_signed_contracts.first.match.flat1 %>
      <% other_user_flat = @last_signed_contracts.first.match.flat2 %>
    <% else %>
      <% current_user_flat = @last_signed_contracts.first.match.flat2 %>
      <% other_user_flat = @last_signed_contracts.first.match.flat1 %>
    <% end %>

